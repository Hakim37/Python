    "def testgen(index):\n",
    "    weekdays = ['sun','mon','tue','wed','the','fri','sat']\n",
    "    yield weekdays[index]\n",
    "    yield weekdays[index+1]\n",
    "    day = testgen(0)\n",
    "    next(day),next(day)"
   ]
